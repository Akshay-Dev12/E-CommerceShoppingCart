<section>
        <div class="container mt-5">
            <div class="row">
                {{#each products}}
                <div class="col-md-3 p-3">
                    <div class="card" style="width: 15rem;">
                        <img class="card-img-top" src="./pdtImages/{{this._id}}.jpeg" alt="Card image cap">
                        <div class="card-body">
                            <h5 class="card-title">{{this.Name}}</h5>
                            <td>Price</td>
                             <p class="card-text">{{this.Price}}</p>
                           <br> <p class="card-text">{{this.Dis}}</p>
                            <a href="/addToCart/{{this._id}}" class="btn btn-primary">Add to cart</a>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
        </div>
    
    </section>
